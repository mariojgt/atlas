{
    "name": "mariojgt/atlas",
    "description": "A Laravel Package Atlas, a quick start for new laravel Packages",
    "keywords": [
        "laravel",
        "translation",
        "laravel translation",
        "laravel javascript translation",
        "vue laravel javascript translation",
        "vue laravel translation"
    ],
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "mario",
            "email": "mariojgt2@gmail.com"
        }
    ],
    "require":{
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Mariojgt\\Atlas\\": "src"
        }
    },

    "scripts": {
        "post-autoload-dump": [
            "@php artisan route:list"
        ]
    },

    "extra": {
        "laravel": {
            "providers": [
                "Mariojgt\\Atlas\\AtlasProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
